> @@ -199,6 +200,11 @@ static inline int swap_zeromap_batch(swp_entry_t ent=
ry, int max_nr,
>         return 0;
>  }
>
> +static inline int kcompressd(void *p)
> +{
> +       return 0;
> +}
> +
>  #endif /* CONFIG_SWAP */
>
